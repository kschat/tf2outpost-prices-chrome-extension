{
	"manifest_version": 2,
	"name": "tf2outpost price integration",
	"description": "Grabs prices of items from backpack.tf and adds them to tf2outpost.com",
	"version": "1.2.1",
	"icons": {
		"16": "images/keyicon16.png",
		"48": "images/keyicon48.png",
		"128": "images/keyicon128.png"
	},
	"content_scripts": [
		{
			"matches": ["*://*.tf2outpost.com/*"],
			"css": ["css/prices.css"],
			"js": ["js/libs/require/require-2.1.2.min.js", "js/libs/require/require-cs.js", "js/bootstrap.js"]
		}
	],
	"permissions": [
		"contextMenus",
		"storage",
		"http://backpack.tf/*",
		"*://*.tf2outpost.com/*"
	],
	"page_action": {
		"default_icon": {
			"19": "images/keyicon19.png",
			"38": "images/keyicon38.png"
		},
		"default_title": "TF2 Outpost prices",
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"js/*"
	]
}